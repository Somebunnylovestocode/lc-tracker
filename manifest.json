{
  "manifest_version": 3,
  "name": "LeetCode Tracker",
  "version": "1.0",
  "description": "Track noob_ace's progress and show your target.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_title": "LeetCode Tracker"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}

